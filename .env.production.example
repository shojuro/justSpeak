# Production environment variables example
# Copy this file to .env.production and fill in your production values
# These should be set in your Vercel dashboard as environment variables

# OpenAI API Configuration
OPENAI_API_KEY=your_production_openai_api_key

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_production_elevenlabs_api_key

# JWT Secret
JWT_SECRET=your_production_jwt_secret_at_least_32_chars

# Environment
NODE_ENV=production

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_production_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key
DATABASE_URL=your_production_database_url
